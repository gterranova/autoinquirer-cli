{
  "name": "autoinquirer-cli",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run build && node . schema.json data.json",
    "clean": "rimraf coverage build tmp dist",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "webpack": "npm run build && webpack --mode production",
    "serve": "node --inspect=55858 -r ts-node/register ./src/index.ts ..\\autoinquirer-cli",
    "start:watch": "nodemon --exitcrash"
  },
  "author": "Gianpaolo Terranova <gianpaoloterranova@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^13.9.8",
    "nodemon": "^2.0.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-command": "^0.5.0"
  },
  "dependencies": {
    "autoinquirer": "file:../autoinquirer",
    "chalk": "^3.0.0",
    "commander": "^5.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^7.0.0",
    "inquirer-datepicker-prompt": "^0.4.2",
    "moment": "^2.24.0",
    "rxjs": "^6.5.4"
  },
  "nodemonConfig": {
    "restartable": false,
    "exitcrash": false,
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "../autoinquirer/build",
      "*.json"
    ],
    "exec": "npm run serve",
    "ext": "ts"
  }
}
